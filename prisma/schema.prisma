generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Sensor {
  id    Int     @id @default(autoincrement())
  nome  String
  valor String
}

model Usuario {
  id      Int      @id @default(autoincrement())
  nome    String
  culturas Cultura[]
}

model Cultura {
  id   Int     @id @default(autoincrement())
  nome String
  kc   Float
  usuario_id Int
  usuario    Usuario @relation(fields: [usuario_id], references: [id])
}